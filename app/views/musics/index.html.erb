<div >
  <% @musics.each do |music| %>
    <iframe src="https://open.spotify.com/embed/track/<%= music.spotify_music_id %>" width="100%" height="80" frameborder="0" allowtransparency="true" allow="encrypted-media"></iframe>
    <p>
      <%= link_to "Show this music", music %>
    </p>
  <% end %>
</div>